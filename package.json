{
  "name": "braille-tutor-app",
  "version": "1.0.0",
  "description": "AI-Powered Braille Learning Platform with Interactive Tutoring",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build:all": "eas build --platform all",
    "submit:android": "eas submit --platform android",
    "submit:ios": "eas submit --platform ios",
    "postinstall": "node scripts/fix-android-manifests.js"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@google/generative-ai": "^0.21.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-voice/voice": "^3.2.4",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@react-navigation/stack": "^6.3.20",
    "@reduxjs/toolkit": "^2.3.0",
    "@supabase/supabase-js": "^2.45.4",
    "axios": "^1.6.2",
    "canvas": "^3.2.0",
    "date-fns": "^2.30.0",
    "expo": "~54.0.33",
    "expo-av": "~16.0.7",
    "expo-blur": "~15.0.7",
    "expo-build-properties": "~1.0.9",
    "expo-font": "^14.0.11",
    "expo-haptics": "~15.0.7",
    "expo-linear-gradient": "~15.0.7",
    "expo-speech": "~14.0.4",
    "expo-splash-screen": "~31.0.11",
    "expo-status-bar": "~3.0.8",
    "lottie-react-native": "~7.3.1",
    "react": "19.1.0",
    "react-native": "^0.81.5",
    "react-native-ble-manager": "^10.1.0",
    "react-native-dotenv": "^3.4.11",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-modal": "^13.0.1",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-vector-icons": "^10.0.3",
    "react-native-worklets": "0.5.1",
    "react-redux": "^9.1.0",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-transform-arrow-functions": "^7.27.1",
    "@babel/plugin-transform-block-scoping": "^7.28.5",
    "@babel/plugin-transform-destructuring": "^7.28.5",
    "@babel/plugin-transform-optional-chaining": "^7.28.5",
    "@babel/plugin-transform-parameters": "^7.27.7",
    "@babel/plugin-transform-shorthand-properties": "^7.27.1",
    "@babel/plugin-transform-spread": "^7.27.1",
    "@babel/plugin-transform-template-literals": "^7.27.1",
    "@types/react": "~19.1.10",
    "@types/react-native-vector-icons": "^6.4.18",
    "babel-preset-expo": "^54.0.10",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "react-native-dotenv": "^3.4.11",
    "typescript": "^5.1.3"
  },
  "private": true,
  "keywords": [
    "braille",
    "accessibility",
    "education",
    "AI",
    "assistive-technology"
  ],
  "author": "Braille Tutor Team",
  "license": "MIT",
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": ["@react-native-voice/voice", "react-native-vector-icons"],
        "listUnknownPackages": false
      }
    }
  }
}
